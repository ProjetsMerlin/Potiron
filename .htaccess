<IfModule mod_rewrite.c>
Options +FollowSymlinks
RewriteEngine on

#HTTPS
#RewriteCond %{HTTPS} off
#RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R,L]

#REECRITURE D'URL
#RewriteRule ^fr/$ ?langue=fr [L]
#RewriteRule ^an/$ ?langue=an [L]
#RewriteRule ^nl/$ ?langue=nl [L]

#RewriteRule ^cookies$ pages/cookies.php [L]
#RewriteRule ^privacy-policy$ pages/privacy-policy.php [L]

#RewriteRule ^pages/(.\w*)$ pages/$1.php [R,L]

#RewriteRule ^404$ 404.php [R,L]
#ErrorDocument 404 https://lintermediaire.be/404
</IfModule>